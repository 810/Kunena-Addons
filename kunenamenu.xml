<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install>
<install version="1.5" type="plugin" group="community" method="upgrade">
	<name>JomSocial - Kunena Forum Menu</name>
	<author>Kunena Team</author>
	<authorEmail>kunena@kunena.com</authorEmail>
	<authorUrl>http://www.kunena.com</authorUrl>
	<creationDate>@kunenaversiondate@</creationDate>
	<build>@kunenaversionbuild@</build>
	<copyright>(c) 2009, 2010 Kunena Team, All rights reserved</copyright>
	<license>GNU/GPL</license>
	<version>1.6.0-RC1</version>
	<isapplication>true</isapplication>
	<isbrowseable>true</isbrowseable>
	<description>PLG_COMMUNITY_KUNENAMENU_DESC</description>
	<languages>
		<language tag="en-GB">en-GB.plg_community_kunenamenu.ini</language>
	</languages>
	<files>
		<file plugin="kunenamenu">kunenamenu.php</file>
		<folder>kunenamenu</folder>
	</files>
	
	<params>
		<param name="coreapp" type="list" default="0" label="PLG_COMMUNITY_KUNENAMENU_PARAM_COREAPP" description="PLG_COMMUNITY_KUNENAMENU_PARAM_COREAPP_DESC">
			<option value="0">PLG_COMMUNITY_KUNENAMENU_OPTION_NO</option>
			<option value="1">PLG_COMMUNITY_KUNENAMENU_OPTION_YES</option>
		</param>
	</params>
</install>
